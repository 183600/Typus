	fmt.Printf("Heap idle: %d bytes\n", m.HeapIdle)
	fmt.Printf("Heap in use: %d bytes\n", m.HeapInuse)
	
	// GC target
	debug.SetGCPercent(50)
	fmt.Println("Set GC target to 50%")
}

